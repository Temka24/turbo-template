{
    "name": "first",
    "private": true,
    "scripts": {
        "build": "turbo run build",
        "dev": "turbo run dev",
        "lint": "turbo run lint",
        "format": "prettier --write \"**/*.{ts,tsx,md}\"",
        "check-types": "turbo run check-types",
        "prepare": "husky",
        "generate": "prisma generate --schema=packages/db/prisma/schema.prisma"
    },
    "devDependencies": {
        "@swc/helpers": "0.5.17",
        "husky": "^9.1.7",
        "lint-staged": "^16.1.2",
        "lodash.get": "^4.4.2",
        "prettier": "^3.5.3",
        "prisma": "^6.12.0",
        "turbo": "^2.5.4",
        "typescript": "5.8.2"
    },
    "packageManager": "pnpm@10.13.1",
    "engines": {
        "node": ">=18"
    },
    "resolutions": {
        "@swc/helpers": "0.5.17",
        "glob": "^10.3.10"
    },
    "pnpm": {
        "overrides": {
            "rimraf": "^5.0.0",
            "lodash.get": "^4.4.2"
        },
        "ignoredBuiltDependencies": [
            "@prisma/client",
            "@prisma/engines",
            "@scarf/scarf",
            "prisma",
            "unrs-resolver"
        ],
        "onlyBuiltDependencies": [
            "@nestjs/core",
            "@swc/core",
            "@tailwindcss/oxide",
            "core-js-pure",
            "sharp"
        ]
    },
    "dependencies": {
        "@nestjs/cli": "^11.0.7",
        "@prisma/client": "^6.12.0",
        "react": "18.3.1",
        "react-dom": "18.3.1"
    }
}
